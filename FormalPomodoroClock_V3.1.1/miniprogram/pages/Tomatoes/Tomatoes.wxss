  font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
  font-weight: 100;
  color: #777777;
  display: flex;
  background-color: #f7f7f7;
  justify-content: center;
  align-items: center;
  /* transform: skewX(37deg); */
  border: 0rpx solid rgb(204, 204, 204);
  border-radius: 48rpx;
}
.Cells_Hover{
  width: 83rpx;
  height: 83rpx;
  border: 2rpx solid #61aff8;
  border-radius: 48rpx;
}
.CellContainer{
  width: 700rpx;
