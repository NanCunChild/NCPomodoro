.LogsChart{
  width: 800rpx;
  height: 800rpx;
}
ec-canvas{
  width: 100%;
  height: 100%;
}
.LeftArrow{
    width: 14px;
    height: 14px;
    border-top: 2px solid #999;
    border-right: 2px solid #999;
    transform: rotate(225deg);
    margin:10rpx 20rpx;
}
.RightArrow{
  width: 14px;
  height: 14px;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  transform: rotate(45deg);
  margin:10rpx 20rpx;
}
.CalenderBarDate{
  font-size: 42rpx;
  font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
  font-weight: 100;
}
.WeekTC{
  width: 84rpx;
  height: 20rpx;
  margin: 0rpx 5rpx 0rpx 5rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 18rpx;
  color: #777777;

  /* border: #777777 solid 2rpx; */
}
.WeekTCContainer{
  width: 640rpx;
  height: 24rpx;
  margin: 0rpx 0rpx -18rpx 0rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: nowrap;

  /* border: #777777 solid 2rpx; */
}
.Cell{
  width: 86rpx;
  height: 86rpx;
  margin:5rpx;
  font-size: 36rpx;
  font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
  font-weight: 100;
  display: flex;
  background-color: rgb(245, 243, 245);
  justify-content: center;
  align-items: center;
  /* transform: skewX(37deg); */
  border: 0rpx solid rgb(204, 204, 204);
  border-radius: 48rpx;
}

.Cell_ToDay{
  width: 82rpx;
  height: 82rpx;
  margin:5rpx;
  font-size: 36rpx;
  font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
  font-weight: 100;
  text-decoration: underline;
  color: #61aff8;
  display: flex;
  background-color: rgb(245, 243, 245);
  justify-content: center;
  align-items: center;
  /* transform: skewX(37deg); */
  border: 4rpx solid  #519fe7;
  border-radius: 48rpx;
}
.Cell_ToDayHover{
  width: 86rpx;
  height: 86rpx;
  color: #fefefe;
  background-color: #61aff8;
  border: 0rpx solid #519fe7;
}
.Cell_RestDay{
