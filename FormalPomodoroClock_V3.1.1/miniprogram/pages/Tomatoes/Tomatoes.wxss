}
.DetailInfo_FIXED{
  width: 360rpx;
  height: 480rpx;
  background-color: #fcfcfc;
  position: fixed;
  flex-direction: column;
  align-items: center;
  border: #dbdbdb solid 2rpx;
  border-radius: 21rpx;
  transition: All 0.4s ease;
  z-index: 2;
}
