/* pages/develop/developmaker.wxss */
page {
  background-color: #e7e7e7;
}

.container {
  width: 100%;
  height: auto;
  margin-bottom: 80rpx;
  margin-top: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}

.rank-item {
  height: auto;
  width: 720rpx;
  background-color: #f5f5f5;
  margin-bottom: 8rpx;
  padding: 20rpx 20rpx 20rpx 30rpx;
  box-sizing: border-box;
  position: relative;
  left: 0rpx;
  border-radius: 20rpx;
  box-shadow: rgba(173, 173, 173, 0.445) 5rpx 5rpx 10rpx;
}

.me {
  background-color: #dfdfdf;
}

.rank-item-First {
  background: linear-gradient(to left, #e4c642, #f5f5f5);
}

.rank-item-Second {
  background: linear-gradient(to left, #bcbece, #f5f5f5);
}

.rank-item-Third {
  background: linear-gradient(to left, #b87333, #f5f5f5);
}

.rank-item .rank-img {
  width: 100rpx;
  height: 100rpx;
  float: left;
  margin-right: 50rpx;
  position: relative;
}

.rank-item .rank-img image {
  position: absolute;
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
  top: 0;
  left: 0;
}

.rank-item .rank-name {
  font-size: 32rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #5c5c5c;
  font-weight: bold;
}

.rank-item .rank-full {
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #5c5c5c;
}

.rank-item .rank-price {
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 10rpx;
  font-size: 21rpx;
  color: #7c7c7c;
}

.rank-item .rank-uv text {
  font-size: 38rpx;
  position: absolute;
  height: 100rpx;
  line-height: 100rpx;
  bottom: 20rpx;
  right: 20rpx;
  color: #777777;
}


.rank-item .rank-uv image {
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  bottom: 20rpx;
  right: 20rpx;
}

.ThreeChosensTime {
  color: black;
  font-weight: 600;
  font-size: 38rpx;
  position: absolute;
  height: 100rpx;
  line-height: 100rpx;
  bottom: 20rpx;
  right: 20rpx;
}

.rank_block {
  margin-top: 10rpx;
}